<!-- 这里面配置一些处理程序的线程数量 -->
<sihan_fk>
	<start_option><!--配置启动线程信息,0表示不启动此线程，1表示启动-->
		<httpserver>1</httpserver><!--http服务器线程-->
		<clientservice>1</clientservice><!--http客户端请求线程，用于向其他地址发起http请求-->
		<hardvideo>0</hardvideo><!--使用硬件编解码读取流-->
		<pullcambricon>1</pullcambricon><!--使用硬件编解码读取流-->
		<readvideo>0</readvideo><!--使用软件编解码读取视频流-->
		<readoneframe>0</readoneframe><!--读取同样的一帧模仿流，主要用于长时间非业务测试-->
		<showimage>0</showimage><!--显示实时处理的某个通道的图像信息，测试的时候使用-->
		<showmap>0</showmap><!--显示地图信息，比如轨迹，测试的时候才使用-->
		<videocontainer>1</videocontainer><!--视频容器，主要是对容器处理前后的存储管理-->
		<trackframe>1</trackframe><!--跟踪线程，此线程开了才能处理视频-->
		<dealoutlist>0</dealoutlist><!--将视频帧释放，不推送到流媒体服务器-->
		<pushoutlist>0</pushoutlist><!--将处理后的视频推送到流媒体服务器jetson-->
		<pushcambricon>1</pushcambricon><!--将处理后的视频推送到流媒体服务器寒武纪-->
		<rpcclient>0</rpcclient><!--远程调用客户端测试用-->	
		<rpcserver>0</rpcserver><!--远程调用服务端预留备用-->
		<imagepushtest>0</imagepushtest><!--空的-->
		<videopushtest>0</videopushtest><!--测试推送图片和警告-->
		<DebugFrame>0</DebugFrame><!--测试线程，开此线程就不开上面的线程-->
		<TrackPushTestRun>0</TrackPushTestRun><!--如果1表示是假轨迹，0表示真轨迹-->
		<CheckFenceInfoRun>1</CheckFenceInfoRun><!--测试发送轨迹消息-->
